<tr>
	<td class="mdl-data-table__cell--non-numeric">
		<%= expense.matter.name if expense.matter %>
	</td>
	<td class="mdl-data-table__cell--non-numeric"><%= expense.desc %></td>
	<td><%= number_to_currency(expense.amount) %></td>
	<td class="delete-link"><%= link_to "Delete", [expense.invoice, expense],
			method: :delete,
			data: { confirm: "Are you sure?"} %>
	</td>
</tr>